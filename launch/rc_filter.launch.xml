<?xml version="1.0"?>
<launch>
  <arg name="fc" default="10.0"/>
  <node pkg="bno055_usb_stick_ros2" exec="bno055_usb_stick_node">
    <param name="port" value="/dev/ttyBNO055"/>
  </node>
  <node pkg="digital_filter" exec="rc_filter_node" output="screen">
    <param name="fc" value="$(var fc)"/>
  </node>
</launch>
